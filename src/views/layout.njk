{% if not isReadme %}
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>GOV.UK Frontend</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/public/css/govuk-frontend.css">
  <!--[if IE 8]>
  <link rel="stylesheet" href="/css/govuk-frontend-oldie.css">
  <![endif]-->
  {% block styles %}
  {% endblock %}
</head>
<body class="{% block bodyClasses %}{% endblock %}">
  <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
{% endif %}
<div class="govuk-o-wrapper">
  {% block content %}
  {% endblock %}
</div>
{% if not isReadme %}
  <script src="/public/js/govuk-frontend.js"></script>
  {% block scripts %}
  {% endblock %}
</body>
</html>
{% endif %}
